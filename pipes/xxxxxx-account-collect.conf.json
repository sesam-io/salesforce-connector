{
  "_id": "xxxxxx-account-collect",
  "type": "pipe",
  "source": {
    "type": "dataset",
    "dataset": "xxxxxx-account-all"
  },
  "transform": [{
    "type": "template",
    "properties": {
      "operation_lookup_delete": "account-lookup",
      "primary_key": "Id",
      "rest_system": "xxxxxx",
      "share_dataset": "xxxxxx-account-share"
    },
    "template": "transform-collect-rest"
  }, {
    "type": "dtl",
    "rules": {
      "default": [
        ["copy", "*"],
        ["add", "$last-modified",
          ["datetime-parse", "%Y-%m-%dT%H:%M:%S.%f%z", "_S.LastModifiedDate"]
        ]
      ]
    }
  }],
  "namespaced_identifiers": false
}
